  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=10.14.1"
  },
  "workspaces": [
    "website",
    "e2e",
    "packages/*"
  ],
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
